{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "functions": {
        "/api/app.py": {
          "runtime": "python3.8",
          "includeFiles": "app.py",
          "maxDuration": 20
        }
    }
  }
  